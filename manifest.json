{
  "manifestVersion": 4,
  "id": "ColorPalette",
  "name": "CustomColorPalette",
  "version": "1.0.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "23.0.0"
  },
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "launchProcess": {
      "schemes": [
        "http",
        "https"
      ],
      "extensions": [
        ".svg",
        ".png"
      ]
    }
  },
  "batchPlay": {
    "version": "1.0.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "Panel",
      "label": {
        "default": "LinearColorPalette"
      },
      "minimumSize": {
        "width": 200,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 600,
        "height": 500
      },
      "preferredFloatingSize": {
        "width": 600,
        "height": 500
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/LinearColorPickerIcon.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/LinearColorPickerIcon.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/LinearColorPickerIcon48.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "darkest"
      ],
      "species": [
        "generic"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/LinearColorPickerIcon48.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ],
      "species": [
        "generic"
      ]
    }
  ]
}